    <body>
        <div class="limiter">
            <div class="container-login100">
                <div class="wrap-login100">
                    <form class="login100-form validate-form" action="MainController" method="POST">
                        <span class="login100-form-title p-b-34">
                            <h1>LOGIN PAGE</h1><br/><br/>
                        </span>
                        <div class="wrap-input100 rs1-wrap-input100 validate-input m-b-20" data-validate="Type user name">
                            <input id="first-name" class="input100" type="text" name="txtUsername" placeholder="Username" value="${requestScope.USERNAME}"><br/>
                            <font color ="red">${requestScope.INVALID.usernameError}</font><br/>
                            <span class="focus-input100"></span>
                        </div>
                        <div class="wrap-input100 rs2-wrap-input100 validate-input m-b-20" data-validate="Type password">
                            <input class="input100" type="password" name="txtPassword" placeholder="Password"><br/>
                            <font color ="red">${requestScope.INVALID.passwordError}</font><br/>
                            <span class="focus-input100"></span>
                        </div>
                        <div class="container-login100-form-btn">

                            <input type="submit" value="Login" name="action" class="login100-form-btn"/><br/><br/>
                        </div>
                            <h4><br/>Â© 2020 by khanghv</h4>
                    </form>
                    <div class="login100-more" style="background-image: url('images/bg-01.jpg');"></div>
                </div>
            </div>
        </div>
<div id="dropDownSelect1"></div>
	
<!--===============================================================================================-->
	<script src="vendor/jquery/jquery-3.2.1.min.js"></script>
